{
  "name": "tennis-ladder-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "no-cache"
      },
      "dest": "/sw.js"
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/club-logo.png",
      "dest": "/club-logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}